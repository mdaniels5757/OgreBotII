    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Get gallery names",
            "program": "${workspaceFolder}/js/exe/get-gallery-names.js",
            "cwd": "${workspaceFolder}/js",
            "runtimeExecutable": "/usr/bin/nodejs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Compile JS",
            "program": "${workspaceFolder}/js/exe/compile-js.js",
            "cwd": "${workspaceFolder}/js",
            "runtimeExecutable": "/usr/bin/nodejs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update old galleries",
            "program": "${workspaceFolder}/js/exe/update-old-galleries.js",
            "cwd": "${workspaceFolder}/js",
            "runtimeExecutable": "/usr/bin/nodejs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Add wikidata categories",
            "program": "${workspaceFolder}/js/exe/add-wikidata-categories.js",
            "cwd": "${workspaceFolder}/js",
            "runtimeExecutable": "/usr/bin/nodejs",
            "skipFiles": [
                "node_modules/**/*.js",
                "_http_agent.js",
                "_http_client.js",
                "_http_common.js",
                "_http_incoming.js",
                "_http_outgoing.js",
                "_stream_duplex.js",
                "_stream_passthrough.js",
                "_stream_readable.js",
                "_stream_transform.js",
                "_stream_writable.js",
                "_tls_common.js",
                "_tls_wrap.js",
                "assert.js",
                "async_hooks.js",
                "buffer.js",
                "buffer_list.js",
                "console.js",
                "constructor.js",
                "debuglog.js",
                "destroy.js",
                "dns.js",
                "errors.js",
                "events.js",
                "fixed_queue.js",
                "freelist.js",
                "helpers.js",
                "http.js",
                "https.js",
                "inspect.js",
                "inspector_async_hook.js",
                "legacy.js",
                "loader.js",
                "net.js",
                "next_tick.js",
                "node.js",
                "pre_execution.js",
                "primordials.js",
                "promises.js",
                "punycode.js",
                "state.js",
                "stdio.js",
                "stream_base_commons.js",
                "task_queues.js",
                "timers.js",
                "types.js",
                "url.js",
                "util.js",
                "utils.js",
                "validators.js",
                "zlib.js"
              ]
        }
    ]
}